class Solution {
    public int removeDuplicates(int[] nums) {
      
      int temp=1;
        for(int i=1;i<nums.length;i++)
        {
          if(nums[i]!=nums[i-1])
          {
            nums[temp]=nums[i];
            temp++;
          }
          
        }
      System.out.println(temp);
      return temp;
    }
}
