class Solution {
    public String removeDuplicates(String s) {
      Stack<Character>stk=new Stack<>();
       StringBuilder str= new StringBuilder();
      for(int i=s.length()-1;i>=0;i--)
      {
        if(stk.empty()||stk.peek()!=s.charAt(i))
        {
          stk.push(s.charAt(i));
        }
        else
        {
          stk.pop();
        }
      }
      while(!stk.empty())
      {
        str.append(stk.pop());
      }
        return str.toString();
      
    }
  

}
