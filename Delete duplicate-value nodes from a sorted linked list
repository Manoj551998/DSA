// Write your code here
    SinglyLinkedListNode head=llist;
    if(head==null)
    {
        return null;
    }
    while(head!=null&&head.next!=null)
    {
        if(head.data==head.next.data)
        {
            head.next=head.next.next;
            
        }
        else
        {
            head=head.next;
        }
    }
        return llist;
