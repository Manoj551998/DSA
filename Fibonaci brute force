import java.util.*;
import java.lang.*;
import java.io.*;

/* Name of the class has to be "Main" only if the class is public. */
class Codechef
{
    
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner sc=new Scanner(System.in);
		 int n=sc.nextInt();
		 if(n<2)
		 {
		 System.out.print(n);
		 }
		 else
		 {
		 int a=0;
		 int b=1;
		 int c=0;
		 for(int i=2;i<=n;i++)
		 {
		     c=a+b;
		     a=b;
		     b=c;
		     
		 }
		 System.out.println(c);
		 }
	}
}
